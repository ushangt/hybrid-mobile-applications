background: -o-radial-gradient(center, ellipse cover,  #42b2c6 0%,#019bb6 99%); /* Opera 12+ */
background: -ms-radial-gradient(center, ellipse cover,  #42b2c6 0%,#019bb6 99%); /* IE10+ */
background: radial-gradient(ellipse at center,  #42b2c6 0%,#019bb6 99%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#42b2c6', endColorstr='#019bb6',GradientType=1 ); /* IE6-8 fallback on horizontal gradient */	
	}
